ovUpJSBachBWVnineninenineArpmod = \relative c' {
  \clef treble
  \key c \minor
  \time 3/4
  r16^\markup{J. S. Bach - BWV 999} c ees g ees c ees c r c r c^\markup{(x 2)}
  r16 c f aes f c f c r c r c^\markup{(x 2)}
  r16 b d f b, d b d r b r b^\markup{(x 2)}
  r16 g c ees
}

ovLowJSBachBWVnineninenineArpmod = \relative c {
  \clef bass
  \key c \minor
  \time 3/4
  c4 r g'8 ees
  c4 r aes'8 f
  c4 r aes'8 f
  c4 
}
